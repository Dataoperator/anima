{
  "assets": {
    "index.html": {
      "path": "dist/index.html",
      "contentType": "text/html",
      "encoding": "gzip"
    },
    "main.html": {
      "path": "dist/main.html",
      "contentType": "text/html",
      "encoding": "gzip"
    },
    "assets/**/*": {
      "path": "dist/assets/**/*",
      "encoding": "gzip"
    }
  },
  "headers": {
    "**/*": {
      "Content-Security-Policy": "default-src 'self' 'unsafe-eval' 'unsafe-inline' data: https://*.icp0.io https://*.ic0.app; img-src 'self' data: https://*.ic0.app https://*.icp0.io; connect-src 'self' https://*.ic0.app https://*.icp0.io https://*.raw.icp0.io",
      "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
      "X-Frame-Options": "DENY",
      "X-Content-Type-Options": "nosniff",
      "Referrer-Policy": "strict-origin",
      "Permissions-Policy": "geolocation=(), microphone=(), camera=()",
      "Cache-Control": "public, max-age=31536000"
    }
  }
}